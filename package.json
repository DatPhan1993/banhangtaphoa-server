{
  "name": "pos-system",
  "version": "1.0.0",
  "description": "Point of Sale System with React Frontend and Node.js Backend",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run server:build && npm run client:build",
    "start": "npm run server:start",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm start",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "deploy:vercel": "./deploy-vercel.sh",
    "deploy:railway": "./deploy-railway.sh",
    "deploy:vps": "./deploy-vps.sh",
    "setup:server": "./server-setup.sh"
  },
  "keywords": ["pos", "react", "nodejs", "mysql", "typescript"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
